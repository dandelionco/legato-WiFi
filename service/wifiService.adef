sandboxed: false

executables:
{
    wifiService = ( ../service/daemon )
}

start: auto

processes:
{
    envVars:
    {
        LE_LOG_LEVEL = DEBUG
    }

    run:
    {
        (wifiService)
    }

    faultAction: restart
}

bindings:
{
}

requires:
{
    configTree: { [r] system }
}

provides:
{
    api:
    {
        wifiService.daemon.le_wifiAp
        wifiService.daemon.le_wifiClient
    }
}
