sandboxed: false

executables:
{
    wifiService = ( ../wifiService/wifiService )
}

start: auto

processes:
{
    envVars:
    {
        LE_LOG_LEVEL = DEBUG
    }

    run:
    {
        (wifiService)
    }

    faultAction: restart
}

bindings:
{
}

requires:
{
    configTree: { [r] system }
}

provides:
{
    api:
    {
        wifiService.wifiService.le_wifiAp
        wifiService.wifiService.le_wifiClient
    }
}
